$ar[$ar.length] = ["hey", "he,re", 2, " aa "];

_writeCSVFile($ar, "D:/sahicsv.txt", true);

var $ar2 = _readCSVFile("D:/sahicsv.txt");

for (var $i=0; $i<$ar.length; $i++){
	for (var $j=0; $j<$ar[$i].length; $j++){
		_assertEqual($ar[$i][$j], $ar2[$i][$j]);
	}
}

